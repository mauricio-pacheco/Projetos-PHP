			<input type="checkbox" name="jobs[]" value="%%GLOBAL_JobName%%" %%GLOBAL_JobDisabled%% />
		</td>
		<td align="Center" style="width:18px;">
			<img src='images/%%GLOBAL_JobType%%.gif' />
		</td>
		<td>
			%%GLOBAL_JobURL%%
		</td>
		<td>
			%%GLOBAL_JobStatus%%
		</td>
		<td>
			%%GLOBAL_JobLinks%%
		</td>
	</tr>
